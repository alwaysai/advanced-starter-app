{
  "scripts": {
    "start": "python app.py"
  },
  "models": {
    "alwaysai/mobilenet_ssd": 5
  },
  "app_configurations": {
    "video_streams": [
      {
        "mode": "USB",
        "arg": 0
      }
    ],
    "inference": {
      "mode": "INFERENCE",
      "confidence": 0.5,
      "overlap_threshold": 0.3
    },
    "tracker": {
      "max_distance": 100,
      "deregister_frames": 30,
      "min_inertia": 6
    },
    "video_writer": {
      "enable": true,
      "output_path": "output.mp4",
      "fps": 30,
      "codec": "avc1",
      "chunk_duration_s": 30
    }
  },
  "analytics": {
    "enable_file_publish": true,
    "file_size_bytes": 500000000
  }
}